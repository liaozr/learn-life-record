.app-text-badge {
	background-image: -webkit-gradient(linear, left bottom, left top, from(#02B360), to(#7BE8B5));
	background-image: -o-linear-gradient(bottom, #02B360 0%, #7BE8B5 100%);
	background-image: linear-gradient(0deg, #02B360 0%, #7BE8B5 100%);
	border-radius: 3rpx;
	margin-right: 16rpx;
	height: 28rpx;
	width: 6rpx;
}
.app-left-text1 {
	font-size: 30rpx;
	color: #000000;
	font-weight: bold;
}
.app-left-text2 {
	font-size: 30rpx;
	color: #000000;
}
.app-right-text1 {
	font-size: 30rpx;
	color: #000000;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	text-align: right;
}
.app-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	padding: 28rpx 40rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.app-item-active {
	background: #eee;
}
.app-save-btn {
	background: #02B360;
	color: white;
	border-radius: 12rpx;
	font-size: 32rpx;
	width: 84%;
	left: 8%;
	line-height: 90rpx;
	bottom: 40rpx;
	position: fixed;
}
.app-btn-active {
	opacity: 0.7;
}
.uni-icon-arrowdown,.uni-icon-arrowright {
	margin-left: 20rpx;
	font-size: 40rpx !important;
}
.divider1 {
	border-bottom: 20rpx solid #F8F8F8;
}
.divider2 {
	border-bottom: 1rpx solid #DDDDDD;
}
.divider3 {
	border-bottom: 1rpx solid #ECEEF5;
}
.flex {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.flex1 {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.red {
	color: red !important;
}
.green {
	color: #02B360 !important;
}
.blod {
	font-weight: bold !important;
}
text{
	font-size: 30rpx;
}
page{
	height: 100%;
}
.app-page{
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	overflow: hidden;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
}
.abs-center{
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%,-50%);
	    -ms-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
}
